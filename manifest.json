{
    "manifest_version": 3,
    "name": "EI Links",
    "version": "0.0.2",

    "action": {
        "default_icon": {
          "16": "media/icons/icon16.png",
          "24": "media/icons/icon24.png",
          "32": "media/icons/icon32.png"
        },
        "default_title": "EI Links",
        "default_popup": "popup.html"
      },
    "description": "Links utiles para un estudiante de Informatica",
    "icons": {
        "16": "media/icons/icon16.png",
        "32": "media/icons/icon32.png",
        "48": "media/icons/icon48.png",
       "128": "media/icons/icon128.png"
    },

    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "permissions": []
}